{
  "name": "website",
  "version": "1.0.0",
  "description": "Portfolio",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "better-npm-run compile",
    "start": "better-npm-run dev",
    "deploy": "better-npm-run deploy",
    "webpack": "webpack --config ./config/webpack.config --output-path ./dist"
  },
  "betterScripts": {
    "compile": {
      "command": "npm run webpack",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "node run ./server/dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomnlittle/website.git"
  },
  "author": "Thomas Northall-Little",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomnlittle/website/issues"
  },
  "homepage": "https://github.com/tomnlittle/website#readme",
  "dependencies": {
    "chalk": "^2.3.0",
    "file-loader": "^1.1.5",
    "fs": "0.0.1-security",
    "fs-extra": "^4.0.2",
    "moment": "^2.19.2",
    "node-sass": "^4.6.1",
    "nodemon": "^1.12.1",
    "react": "^16.1.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.6",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "react-hot-loader": "^3.1.2",
    "webpack-dev-server": "^2.9.4"
  }
}
